@model IEnumerable<Microsoft.AspNet.Identity.EntityFramework.IdentityUser>

<h2>Delete User</h2>
@using (Html.BeginForm("delete", "Shared", FormMethod.Post, new { id = "popupForm" }))
{
    @Html.HiddenFor(a => a.Id)
    <div class="form-group">
        <label>UserName</label>
        <p>@Model.UserName</p>
    </div>

    <div class="form-group">
        <label>PhoneNumber</label>
        <p>@Model.PhoneNumber</p>
    </div>

    <div class="form-group">
        <label>Email</label>
        <p>@Model.Email</p>
    </div>

    @*<div class="form-group">
        <label>City</label>
        <p>@Model.City</p>
    </div>

    <div class="form-group">
        <label>Country</label>
        <p>@Model.Country</p>
    </div>*@

    <div>
        <input type="submit" value="Delete" />
    </div>
}
